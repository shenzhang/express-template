<!DOCTYPE html>
<html lang="en" ng-app="index">
<head>
  <meta charset="UTF-8">
  <% include template/css.ejs %>
  <% include template/js.ejs %>
</head>

<body ng-controller="IndexController">
  <input type="text" ng-model="hello"/>

  <div>{{hello}}</div>
  
</body>
<script>
  var index = angular.module('index', []);

  index.controller('IndexController', function($scope) {
    $scope.hello = 'world';
  });
</script>
</html>